<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jc.mapper.DadCommentMapper">

    <select id="selectByPostId" parameterType="Integer" resultType="com.jc.entity.pojo.Dadcomment">
        select *
        from dadcomment where post_id=#{postId}
    </select>


</mapper>